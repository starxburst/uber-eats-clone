    "expo": "^46.0.0",
    "expo-status-bar": "~1.4.0",
    "firebase": "^9.9.2",
    "lottie-react-native": "^5.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.3",
    "react-native-bouncy-checkbox": "^3.0.2",
    "react-native-device-info": "^10.0.2",
